    "Datepicker",
    "DateField",
    "DateInput"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/karianpour/react-hichestan-datetimepicker"
  },

  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build-examples": "react-scripts build",
