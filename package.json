    "react": "^16.5.2",
    "react-doc-generator": "^1.2.5",
    "react-dom": "^16.5.2",
    "react-scripts": "^1.1.5",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "moment-jalaali": "^0.7.4",
    "shallow-equal": "^1.0.0"
  }
